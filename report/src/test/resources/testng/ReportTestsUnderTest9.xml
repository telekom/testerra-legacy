<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Before Scenario" verbose="1" parallel="methods" thread-count="10" data-provider-thread-count="10" configfailurepolicy="continue">

    <test name="Before Test Failed">
        <groups>
            <run>
                <include name="SYSTEMTESTSFILTER9"/>
                <include name="TEST"/>
            </run>
        </groups>
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.report.testundertest.ReportTestUnderTestBeforeScenarios"/>
        </classes>
    </test>

    <test name="Before Groups Failed">
        <groups>
            <run>
                <include name="SYSTEMTESTSFILTER9"/>
                <exclude name="TEST"/>
                <include name="GROUPS"/>
                <exclude name="CLASS"/>
            </run>
        </groups>
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.report.testundertest.ReportTestUnderTestBeforeScenarios"/>
        </classes>
    </test>

    <test name="Before Class Failed">
        <groups>
            <run>
                <include name="SYSTEMTESTSFILTER9"/>
                <exclude name="TEST"/>
                <exclude name="GROUPS"/>
                <include name="CLASS"/>
            </run>
        </groups>
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.report.testundertest.ReportTestUnderTestBeforeScenarios"/>
        </classes>
    </test>

    <test name="Before Method Failed">
        <groups>
            <run>
                <include name="SYSTEMTESTSFILTER9"/>
                <exclude name="TEST"/>
                <exclude name="GROUPS"/>
                <exclude name="CLASS"/>
                <include name="METHOD"/>
            </run>
        </groups>
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.report.testundertest.ReportTestUnderTestBeforeScenarios"/>
        </classes>
    </test>

    <test name="Before Run All">
        <groups>
            <run>
                <include name="SYSTEMTESTSFILTER9"/>
                <exclude name="TEST"/>
                <exclude name="GROUPS"/>
                <exclude name="CLASS"/>
                <exclude name="METHOD"/>
            </run>
        </groups>
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.report.testundertest.ReportTestUnderTestBeforeScenarios"/>
        </classes>
    </test>

</suite>
